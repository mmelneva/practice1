{
    "name": "diol/laravel-mailer",
    "description": "Extended mailer for Laravel 4.2",
    "authors": [
        {
            "name": "Andrey Pokoev",
            "email": "andrej.pokoev@diol-it.ru"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/mail": "4.2.*",
        "illuminate/queue": "4.2.*",
        "illuminate/support": "4.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.9.*",
        "orchestra/testbench": "2.2.*"
    },
    "autoload": {
        "psr-4": {
            "Diol\\LaravelMailer\\": "src/Diol/LaravelMailer",
            "tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-default": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable"
}
